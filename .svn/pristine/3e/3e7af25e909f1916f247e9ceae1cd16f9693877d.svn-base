.termektext
{
	background: rgba(0,0,0,0.5);
	display: table;
	position: absolute;
    top: 190px;
	left: 0px;
	right: 0px;
	height: 100px;
	opacity: 0;
	-webkit-transition: opacity 500ms;
	-moz-transition: opacity 500ms;
	-o-transition: opacity 500ms;
	transition: opacity 500ms;
	color: #FFF;
}

.termek:hover .termektext
{
	opacity: 0.85;
}

.boltTermektext
{
	background: rgba(0,0,0,0.5);
	display: table;
	position: absolute;
    top: 190px;
	left: 0px;
	right: 0px;
