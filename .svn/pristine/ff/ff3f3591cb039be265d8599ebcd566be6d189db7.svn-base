span.termektext
{
	background: rgba(0,0,0,0.5);
	color: white;
	cursor: pointer;
	display: table;
	height: 100px;
	left: 0;
	position: absolute;
	top: 190;
	width: 100%;
	opacity: 0;
	-webkit-transition: opacity 500ms;
	-moz-transition: opacity 500ms;
	-o-transition: opacity 500ms;
	transition: opacity 500ms;
}

.termek:hover span.termektext
{
  opacity: 0.8;
}

span.termektext span
{
  display: table-cell;
  text-align: center;
  vertical-align: middle;
}


span.boltTermektext
{
	background: rgba(0,0,0,0.5);
	display: table;
	position: absolute;
    top: 190px;
	left: 0px;
	right: 0px;
	
	height: 100px;
	opacity: 0;
	-webkit-transition: opacity 500ms;
